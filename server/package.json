{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:backend": "SET \"NODE_ENV=development\" SET DEBUG=myapp:* & nodemon ./bin/www",
    "start:backend:mac": "NODE_ENV=development DEBUG=APP_NAME:* nodemon ./bin/www",
    "start:frontend": "npm run start --prefix ../frontend",
    "start:dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:mac": "concurrently \"npm run start:backend:mac\" \"npm run start:frontend\"",
    "db:create:migration": "sequelize migration:generate --name ",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "postinstall": "sequelize db:migrate",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:backend": "cross-env NODE_ENV=development jest --verbose --runInBand --detectOpenHandles"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.8",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "fast-csv": "^4.1.3",
    "http-errors": "~1.6.3",
    "js-combinatorics": "^0.5.5",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "pg-promise": "^10.5.3",
    "pug": "^2.0.4",
    "sequelize": "^5.21.7",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.0.1",
    "nodemon": "^2.0.3",
    "supertest": "^4.0.2"
  }
}
